<%- include("partials/header") -%>


    <h1>Home</h1>


    <p>
        <%= startingContent %>
    </p>

    <% posts.forEach(post=> { %>
        <h2>
            <%= post.title %>
        </h2>
        <p>
            <%= post.content.slice(0, 100)+'...' %>
